{
  "name": "gantt-dashboard",
  "description": "Gantt Dashboard met Bryntum Gantt",
  "version": "1.0.0",
  "author": {
    "name": "Mick"
  },
  "private": true,
  "homepage": "./",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next dev",
    "export": "next export",
    "lint": "next lint",
    "postinstall": "postinstall"
  },
  "postinstall": {
    "node_modules/@bryntum/demo-resources/images/favicon.png": "copy public/favicon.png",
